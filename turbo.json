{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [".angular", "dist"]
    },
    "dev": {
      "cache": false,
      "persistent": true,
      "dependsOn": ["^build"]
    },
    "dev:ssr": {
      "cache": false,
      "persistent": true,
      "dependsOn": ["build"]
    },
    "lint": {},
    "lint:fix": {},
    "preview": {
      "cache": false,
      "persistent": true,
      "dependsOn": ["^build"]
    }
  }
}
