{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [".angular", ".turbo", "dist"]
    },
    "lint": {},
    "lint:fix": {},
    "start": {
      "cache": false,
      "persistent": true,
      "dependsOn": ["^build"]
    },
    "start:ssr": {
      "cache": false,
      "persistent": true,
      "dependsOn": ["build"]
    },
    "test": {}
  }
}
