{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "tasks": {
    "build": {
      "dependsOn": ["^build"]
    },
    "build:prod": {
      "dependsOn": ["^build:prod"]
    },
    "lint": {},
    "lint:fix": {},
    "start": {
      "cache": false,
      "persistent": true,
      "dependsOn": ["^build"]
    },
    "start:prod": {
      "cache": false,
      "persistent": true,
      "dependsOn": ["build:prod"]
    }
  }
}
